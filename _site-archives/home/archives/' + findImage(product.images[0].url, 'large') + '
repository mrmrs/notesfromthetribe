  <script type="text/javascript" charset="utf-8">
      var subdomain = 'notesfromthetribe';
      $(function() {
        function findImage(url, size) {
              if(!size) return url;
              var ext = url.match(/\.(\w{2,4}$)/)[1];
              var dir = url.substr(0, url.lastIndexOf('/') + 1);
              switch(size.toLowerCase()) {
                  case 'large':
                      return dir + '300.' + ext;
                  case 'medium':
                      return dir + '175.' + ext;
                  case 'thumb':
                      return dir + '75.' + ext;
                  default:
                      return url;
              }
        }
        $.getJSON('http://api.bigcartel.com/' + subdomain + '/products.js?callback=?', function(products) {
          $.each(products, function(i, product) {
            $('#products').append('<li class="four columns alpha"><a class="four columns alpha" href="http://' + subdomain + '.bigcartel.com' + product.url + '"><img src="'&#32;+&#32;findImage(product.images[0].url,&#32;'large')&#32;+&#32;'" width="100%" /></a></li>');
            if (i == 5) 
                return false;
          });
        });
      });
    </script>
